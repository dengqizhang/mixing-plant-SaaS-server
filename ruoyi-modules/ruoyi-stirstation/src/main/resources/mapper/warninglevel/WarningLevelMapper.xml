<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dromara.warninglevel.mapper.WaningLevelMapper">
        <select id="selectWarningLevelById" resultType="org.dromara.warninglevel.domin.SysUser">
            select
                u.user_id,u.user_name,s.warninglevel_id
            FROM sys_user u
                LEFT JOIN
                stirstation_user_warninglevel s ON u.user_id = s.user_id
        </select>

</mapper>
